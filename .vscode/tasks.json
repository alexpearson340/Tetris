{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: clang++ build active file",
      "command": "bash",
      "args": [
        "-c",
        "clang++ -g -fcolor-diagnostics -fansi-escape-codes -std=c++17 main.cpp Tetris.cpp Texture.cpp -L /opt/homebrew/opt/sdl2/lib -lSDL2 -L /opt/homebrew/opt/sdl2_image/lib -lSDL2_image -L /opt/homebrew/opt/sdl2_ttf/lib -lSDL2_ttf -I ${workspaceFolder}/ -I /opt/homebrew/opt/sdl2/include/SDL2/ -I /opt/homebrew/opt/sdl2_image/include/SDL2/ -I /opt/homebrew/opt/sdl2_ttf/include/SDL2/ -o ${workspaceFolder}/main"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}